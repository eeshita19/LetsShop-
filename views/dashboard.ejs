<%- include ("./partials/header") %>

<div class="container">
    <header class="jumbotron">
        <div class="container">

            <h1>Welcome to LetsShop!!</h1>
            
            <p> Stay Safe in this Covid-19</p>
            <p><a class=" btn btn-primary btn-lg" href="/campgrounds">Go to all Shops</a></p>
        </div>
    </header>


<div class="row"">
  <% camp.lists.forEach(list => { %>
    <div class="col-sm-6 col-md-4">  
        <div class="thumbnail">
        <img src="<%= list.link %>" alt="..."/>
        <div class="caption">
          <h3><%= list.username %></h3>
          <p>
            <a href="#" class="btn btn-primary" role="button">CONFIRM PACKAGE</a> <a href="#" class="btn btn-danger" role="button">DELETE</a> 
          </p>
        </div>
      </div>
    </div>
  <% }) %>
  </div>

</div>




<%- include ("./partials/footer") %>